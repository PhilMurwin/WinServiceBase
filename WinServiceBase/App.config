<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8"/>
	</startup>

	<appSettings>
		<!-- Determines if win event logger process should be started, no additional settings -->
		<add key="BasicTimeLogger" value="true" />
		<add key="BasicTimeLoggerFrequency" value="1" />

		<!-- Delayed Process -->
		<add key="DelayedProcess" value="true" />
		<add key="DelayedProcessFrequency" value="1" />
		<add key="DelayedProcessDelayStartup" value="true" />

	</appSettings>

</configuration>
