<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  
  <appSettings>
    <!-- Debug-Break flag: if "true" then a debugger breakpoint occurs when the program is starting -->
    <add key="DebugBreak" value="false" />

    <!-- Determines if win event logger process should be started, no additional settings -->
    <add key="WindowsEventLogger" value="true" />
    
  </appSettings>
  
</configuration>